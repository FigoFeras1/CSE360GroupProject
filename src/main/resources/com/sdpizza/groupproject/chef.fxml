<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sdpizza.groupproject.controller.Controller">
   <children>
      <SplitPane dividerPositions="0.4, 0.8" prefHeight="200.0" prefWidth="200.0">
         <items>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ListView fx:id="readyToCookList" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <VBox prefHeight="198.0" prefWidth="51.0">
               <children>
                  <ListView fx:id="cookingList" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button layoutX="58.0" layoutY="44.0" mnemonicParsing="false" text="Start cooking" />
                  <Button layoutX="77.0" layoutY="86.0" mnemonicParsing="false" text="Finish" />
                  <Button layoutX="74.0" layoutY="158.0" mnemonicParsing="false" text="Log out" />
               </children>
            </AnchorPane>
         </items>
      </SplitPane>
   </children>
</VBox>
