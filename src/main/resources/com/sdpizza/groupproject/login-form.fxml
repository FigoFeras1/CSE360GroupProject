            <Insets />
         </opaqueInsets>
         <padding>
            <Insets right="10.0" />
         </padding>
         <children>
            <Button fx:id="homeRegisterButton" mnemonicParsing="false" onAction="#homeRegister" text="Register" />
         </children>
      </VBox>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="352.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" layoutX="126.0" layoutY="71.0" onKeyPressed="#keyPressed" prefHeight="258.0" prefWidth="349.0" spacing="5.0">
