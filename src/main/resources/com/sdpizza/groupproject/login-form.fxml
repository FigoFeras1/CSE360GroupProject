<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sdpizza.groupproject.controller.Controller">
   <children>
      <VBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="600.0" style="-fx-background-color: #8C1D40;">
