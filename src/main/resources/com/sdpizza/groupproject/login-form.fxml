                     </children>
                     <padding>
                        <Insets right="36.0" />
                     </padding>
                  </HBox>
                  <AnchorPane />
                  <Label fx:id="loginText" alignment="BOTTOM_CENTER" prefHeight="18.0" prefWidth="247.0" text="label" visible="false" />
               </children>
            </VBox>
            <VBox layoutX="154.0" layoutY="-234.0" prefHeight="200.0" prefWidth="100.0" />
         </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </AnchorPane>
   </children>
</VBox>
